___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require('logToConsole');
log('data =', data);

// Sandboxed GTM APIs
var setInWindow  = require('setInWindow');
var injectScript = require('injectScript');
var logToConsole = require('logToConsole');

// Hard-coded Babelbeez endpoints
var proxyInitializeUrl = 'https://www.babelbeez.com/initialize-chat';
var loaderUrl          = 'https://www.babelbeez.com/assets/embedLoader.js';

// Read the only field
var publicChatbotId = data.publicChatbotId;

// Helper functions to ensure we always signal completion exactly once
function signalSuccess() {
  if (data.gtmOnSuccess) {
    data.gtmOnSuccess();
  } else if (data.gtmOnFailure) {
    // Fallback so the tag still signals completion
    data.gtmOnFailure();
  }
}

function signalFailure() {
  if (data.gtmOnFailure) {
    data.gtmOnFailure();
  } else if (data.gtmOnSuccess) {
    // Fallback so the tag still signals completion
    data.gtmOnSuccess();
  }
}

// Validate required field
if (!publicChatbotId) {
  logToConsole('Babelbeez Voice Agent: Missing Public Chatbot ID, tag will not run.');
  signalFailure();
  return;
}

// Set or overwrite the global config object on the page
setInWindow(
  'babelbeezConfig',
  {
    publicChatbotId:    publicChatbotId,
    proxyInitializeUrl: proxyInitializeUrl,
    loaderUrl:          loaderUrl
  },
  true // overwrite existing value if present
);

// Load the Babelbeez embed loader script
injectScript(
  loaderUrl,
  function () {
    logToConsole('Babelbeez Voice Agent: Loader script loaded.');
    signalSuccess();
  },
  function (err) {
    logToConsole('Babelbeez Voice Agent: Failed to load loader script.', err);
    signalFailure();
  }
);


